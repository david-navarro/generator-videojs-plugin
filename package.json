{
  "name": "videojs-ra",
  "version": "8.0.0",
  "description": "Richaudience plugin for videojs",
  "license": "UNLICENSED",
  "main": "dist/videojs-ra.cjs.js",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videojs/generator-videojs-plugin.git"
  },
  "author": "navarro <david.navarro@richaudience.com>",
  "vjsstandard": {
    "ignore": [
      "es",
      "cjs",
      "dist",
      "docs",
      "test/dist"
    ]
  },
  "scripts": {
    "build": "npm-run-all -s clean -p build:*",
    "build-prod": "cross-env-shell NO_TEST_BUNDLE=1 'npm run build'",
    "build-test": "cross-env-shell TEST_BUNDLE_ONLY=1 'npm run build'",
    "build:css": "postcss -o dist/videojs-ra.css --config scripts/postcss.config.js src/plugin.css",
    "build:js": "rollup -c scripts/rollup.config.js",
    "build:js:uglify": "uglifyjs dist/videojs-ra.js --comments --mangle --compress  -o dist/videojs-ra.min.js",
    "clean": "shx rm -rf ./dist ./test/dist ./cjs ./es && shx mkdir -p ./dist ./test/dist ./cjs ./es",
    "docs": "npm-run-all docs:*",
    "docs:api": "jsdoc src -r -d docs/api",
    "docs:toc": "doctoc --notitle README.md",
    "generate-pkg-lock": "node scripts/generate-pkg-lock.js && git add plugin/package-lock.json",
    "lint": "vjsstandard",
    "server": "karma start scripts/karma.conf.js --singleRun=false --auto-watch",
    "start": "npm-run-all -p server watch",
    "pretest": "npm run lint",
    "test": "npm-run-all lint build-test && karma start scripts/karma.conf.js",
    "posttest": "shx cat test/dist/coverage/text.txt",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:integration-lib": "node scripts/run-integration-tests.js --library",
    "test:unit": "mocha --timeout 10000 test/*.test.*js",
    "test:verify": "vjsverify --skip-es-check --verbose",
    "update-changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "preversion": "npm test",
    "version": "is-prerelease || npm run update-changelog && git add CHANGELOG.md",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "npm run build:css -- -w",
    "watch:js": "npm run build:js -- -w",
    "prepublishOnly": "npm-run-all build-prod && vjsverify --verbose"
  },
  "keywords": [
    "videojs",
    "videojs-plugin",
    "yeoman-generator"
  ],
  "dependencies": {
    "global": "^4.4.0",
    "lodash": "^4.17.21",
    "please-upgrade-node": "^3.2.0",
    "sync-request": "^6.1.0",
    "uglify-js": "^3.14.5",
    "video.js": "^6 || ^7",
    "videojs-contrib-ads": "^6.9.0",
    "videojs-ima": "^1.11.0",
    "yeoman-environment": "^3.4.1",
    "yeoman-generator": "^5.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@videojs/generator-helpers": "^3.2.0",
    "is-ci": "^3.0.0",
    "jsdoc": "~3.6.6",
    "karma": "^6.3.2",
    "mocha": "^9.1.3",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.46.0",
    "sinon": "^9.1.0",
    "videojs-generate-karma-config": "~8.0.0",
    "videojs-generate-postcss-config": "~3.0.0",
    "videojs-generate-rollup-config": "~6.2.0",
    "videojs-generator-verify": "~3.0.3",
    "videojs-standard": "^9.0.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.1.0"
  },
  "husky": {
    "hooks": {}
  },
  "module": "dist/videojs-ra.es.js",
  "browser": "dist/videojs-ra.js",
  "generator-videojs-plugin": {
    "version": "8.0.0"
  },
  "files": [
    "CONTRIBUTING.md",
    "cjs/",
    "dist/",
    "docs/",
    "es/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "bugs": {
    "url": "https://github.com/videojs/generator-videojs-plugin/issues"
  },
  "homepage": "https://github.com/videojs/generator-videojs-plugin#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
